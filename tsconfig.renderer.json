{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ES2022",
    "lib": [
      "ES2022",
      "DOM"
    ],
    "sourceMap": true,
    "outDir": "out/notebook",
    "rootDir": "src/notebook",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "bundler"
  },
  "include": [
    "src/notebook/renderer.ts"
  ]
}
